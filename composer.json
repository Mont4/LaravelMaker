{
    "name": "mont4/laravel_maker",
    "description": "php artisan make:all for Laravel with Make and generate Laravel 'Controller, Request, Model, Factory, Seed, Migration, Translation, Policy & Permissions' compatible.",
    "license": "license",
    "authors": [
        {
            "name": "Mohammad Montazeri",
            "email": "mo121ntazeri@gmail.com",
            "homepage": "https://github.com/Mont4"
        }
    ],
    "homepage": "https://github.com/Mont4/LaravelMaker",
    "keywords": ["Laravel", "LaravelMaker"],
    "require": {
        "illuminate/support": ">=6.0",
        "spatie/laravel-permission": "~3",
        "mont4/filter": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "mockery/mockery": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Mont4\\LaravelMaker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mont4\\LaravelMaker\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mont4\\LaravelMaker\\ServiceProvider"
            ]
        }
    }
}
